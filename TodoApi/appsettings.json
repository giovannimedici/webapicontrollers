{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "MongoDB": {
    "DatabaseName": "ToDoList",
    "CollectionName": "ToDoList"
  },
  "Collections": {
    "TodoList": "ToDoList",
    "User": "user"
  },
  "JwtSettings": {
    "Issuer": "JwtAuthApi",
    "Audience": "JwtAuthClient",
    "ExpirationMinutes": 60
  },
  "Kestrel": {
  "Endpoints": {
    "Https": {
      "Url": "https://localhost:5001",
      "Certificate": {
        "Path": "cert.pfx",
        "Password": "123456"
      }
    }
  }
}
}
